    "custom": {
      "name": "Custom Plugin",
      "description": "Use your own command or script",
      "license": "User-defined",
      "is_custom": true,
      "user_provides": {
        "command": {
          "required": true,
          "description": "Command to execute (e.g., 'yolo', 'python3 script.py', './binary')",
          "examples": [
            "yolo",
            "python3 my_plugin.py",
            "./detector",
            "docker run myimage"
          ]
        },
        "requirements_file": {
          "required": false,
          "description": "Path to requirements.txt for Python dependencies",
          "note": "Can include git repos: git+https://github.com/user/repo"
        },
        "env_setup": {
          "required": false,
          "description": "Environment setup command (e.g., source venv/bin/activate)"
        }
      },
      "template_detect_args": "detect --image {image} --conf {confidence} --format json",
      "template_train_args": "train --data {data_yaml} --epochs {epochs}",
      "output_format": {
        "description": "Plugin must output JSON to stdout",
        "schema": {
          "detections": [
            {
              "class": "string",
              "class_id": "int",
              "confidence": "float",
              "points": "[[x1, y1], [x2, y2], ...]"
            }
          ]
        }
      }
